{
  "name": "flutter-responsive-browser",
  "version": "1.0.0",
  "description": "A standalone tool that launches a browser window with mobile device emulation for Flutter web development",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "frb": "./bin/frb.js"
  },
  "scripts": {
    "start": "node bin/frb.js",
    "dev": "node --watch bin/frb.js",
    "test": "jest",
    "lint": "eslint src bin --ext .js",
    "lint:fix": "eslint src bin --ext .js --fix"
  },
  "keywords": [
    "flutter",
    "web",
    "browser",
    "responsive",
    "mobile",
    "emulation",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.5.0",
    "chalk": "^5.3.0",
    "electron": "^28.1.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}